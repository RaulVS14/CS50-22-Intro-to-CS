{
  "name": "Chrome Daily Planner",
  "description": "Helps user to plan their day by providing section for scheduled events and for daily tasks",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_png": {
      "16": "./images/calendar.png",
      "32": "./images/calendar.png",
      "48": "./images/calendar.png",
      "128": "./images/calendar.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "/images/calendar.png",
    "32": "/images/calendar.png",
    "48": "/images/calendar.png",
    "128": "/images/calendar.png"
  },
  "permissions": [
    "background",
    "identity",
    "storage",
    "alarms",
    "notifications"
  ],
  "oauth2": {
    "client_id": "403439953383-ubldt0d0ble1lkjb80pbc8e0mneuuv5l.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "key": ""
}